    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "API",
            "preLaunchTask": "compile_ts",
            "program": "${workspaceFolder}/src/index.ts",
            "cwd": "${workspaceFolder}",
            "protocol": "inspector",
            "outFiles": ["${workspaceFolder}/bin/**/*.js"],
            "env": {
                "NODE_ENV": "local-system"
            }
        }
    ]
}
